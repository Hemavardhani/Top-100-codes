import java.util.*;
class Main {
    
    public static int fibo(int arr[],int x)
    {
        int a=0,b=1,c=1;
        int n=arr.length;
        
        while(c<n)
        {
            a=b;
            b=c;
            c=a+b;
        }
        int offset=-1;
       
       
        
        while(c>1)
        {
            int i=Math.min(offset+a,n-1);
            
            if(arr[i]<x)
            {
                c=b;
                b=a;
                a=c-b;
                offset=i;
            }
            else if(arr[i]>x)
            {
                c=a;
                b=b-a;
                a=c-b;
            }
            else
            {
                return i;
            }
        }
         if(b==1 && arr[offset+1]==x)
        {
            return offset+1;
        }
         return -1;
        
    }
        
    
    
    public static void main(String[] args) {
        
        int arr[]={10,22,35,40,45,50,80,82,85,90,100};
        
        int x=90;
        
        int res=fibo(arr,x);
        
        System.out.println("The target elemnet "+x+" is found at the "+res+" position in the given arr");
        System.out.println("Try programiz.pro");
    }
}
